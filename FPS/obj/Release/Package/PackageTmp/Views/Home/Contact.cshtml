@{
    ViewBag.Title = "Contact";
}

<section class="innerBanner">
    <img src="~/img/contact-banner.jpg" alt="">
    <h2 class="float-header">
       Call Us at : 800-123-4567 <br />
        We are waiting for you.
    </h2>
    <div class="float-arrow">
        <img src="~/img/blankArrow.png" />
    </div>
</section>


<section class="innerContent" style="padding-bottom:0px">
  
        <div class="row rowSPCLInnerpages">
            
                @*<h3>Contact Us</h3>*@
             
                    <div class="col-md-6 border-left">
                        <h4>
                            For Dependability You Can Count On<span><span class="skype_c2c_print_container notranslate">800-123-4567</span><span id="skype_c2c_container" class="skype_c2c_container notranslate" dir="ltr" tabindex="-1" onmouseover="SkypeClick2Call.MenuInjectionHandler.showMenu(this, event)" onmouseout="SkypeClick2Call.MenuInjectionHandler.hideMenu(this, event)" onclick="SkypeClick2Call.MenuInjectionHandler.makeCall(this, event)" data-numbertocall="+18007167608" data-numbertype="tollfree" data-isfreecall="true" data-isrtl="false" data-ismobile="false"><span class="skype_c2c_highlighting_inactive_common" dir="ltr" skypeaction="skype_dropdown"><span class="skype_c2c_textarea_span" id="free_num_ui"><img width="0" height="0" class="skype_c2c_logo_img" src="chrome-extension://lifbcibllhkdhoafpjfnlhfpfgnpldfl/call_skype_logo.png"><span class="skype_c2c_text_span">800-716-7608</span><span class="skype_c2c_free_text_span"></span></span></span></span></span>
                        </h4>
                        <p>
                            Freight Pricing Solutions shipping can be complicated and our Customer Service team makes it easy to follow
                            the delivery of your shipment after booking online or by phone. From tracking a delivery to
                            rescheduling a pickup, our team promises to keep you informed every step of the way.
                        </p>
                        <p>
                            We have agents available by phone, chat and email during extended office hours so that we are available when you need us.
                        </p>

                        <hr />
                        <a name="feedback"></a>
                        
                        <br />

                        <h4>
                            You can call us at the below number for emergency services <br/><br/>

                            <span style="color:#444 !important">
                                800-123-4567<br/>
                                800-123-4587<br/>
                                800-123-4557<br/>
                                   1-650-450 0987 (Toll Free)
                            </span>
                            
                        </h4>



                        
                    </div>

                    <div class="col-md-6 Spcl">
                        
                        <h4>
                            Send a Direct Message to Customer Service
                        </h4>
                        <div class="contactForm">
                            <div class="form-group">

                                <input name="txtFullName" type="text" maxlength="46" id="txtFullName" class="form-control form-control-fps" placeholder="Type your name...">
                                <span id="rfvFullName" style="color:Red;display:none;" class="field-validation-valid text-danger " data-valmsg-replace="true"> Required</span>

                            </div>
                            <div class="form-group">
                                <input name="txtEmailAddress" type="text" maxlength="49" id="txtEmailAddress" class="form-control form-control-fps" placeholder="Type your email...">
                                <span id="revEmailAddress" style="color:Red;display:none;" class="field-validation-valid text-danger">Email format must be: Person@address.domain</span>
                                <span id="rvfEmailAddress" style="color:Red;display:none;" class="field-validation-valid text-danger"> Required</span>
                            </div>
                            <div class="form-group">
                                <input name="txtPhoneNumber" type="text" maxlength="12" size="12" id="txtPhoneNumber" class="form-control form-control-fps" placeholder="Phone no...">
                                <span id="rfvPhoneNumber" style="color:Red;display:none;" class="field-validation-valid text-danger"> Required</span>
                            </div>
                            <div class="form-group">
                                <input name="txtShipmentID" type="text" maxlength="12" size="12" id="txtShipmentID" class="form-control form-control-fps" placeholder="Shipment Id...">
                                <span id="rfvShipmentID" style="color:Red;display:none;" class="field-validation-valid text-danger"> Required</span>
                            </div>

                            <div class="form-group">
                                <textarea name="txtMessage" rows="4" cols="20" id="txtMessage" class="form-control form-control-fps" placeholder="Type your message..."></textarea>
                            </div>

                            <div class="form-group">

                                <input type="submit" name="btnSendEmail" class="btn-Standard btn-normal-full-with" value="Send" onclick="javascript:WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions(&quot;btnSendEmail&quot;, &quot;&quot;, true, &quot;&quot;, &quot;&quot;, false, false))" id="btnSendEmail">
                            </div>
                        </div>
                    </div>

             
            
        </div>

    <section class="contactBg lessSpace">
        <div class="container">
            <div class="row rowSPCLInnerpages">
                <div class="col-md-12">
                    <div class="bgContent">
                        <h2>Freight Pricing Solution <br/> Headquarters</h2>
                        <h4>Superior service, innovation & technology since 1998.</h4>
                        <div class="addressBox">
                            <h3>
                                
                            </h3>

                            <p>

                                12 Penns Trail, Suite 12200, <br />
                                Newtown PA 18940<br />
                                Phone : <strong>  800 123 45678 900 1234 FREE </strong><br />



                                Fax: 727 450 9999


                            </p>



                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <iframe  src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3046.076594839413!2d-74.91117358511158!3d40.229599174924005!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89c15443325df061%3A0x4cb9ee0ecea0ab1d!2s12+Penns+Trail%2C+Newtown%2C+PA+18940%2C+USA!5e0!3m2!1sen!2sin!4v1468509530230" width="100%" height="500" frameborder="0" allowfullscreen></iframe>


  
</section>








